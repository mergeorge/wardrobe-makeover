<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

<xs:element name="service">
	<xs:complexType>
	    <xs:sequence>
	        <xs:element name="booking" maxOccurs="unbounded">
	        	<xs:complexType>
				    <xs:sequence>
						<xs:element name="id" type="xs:integer"/>
						<xs:element name="category" type="xs:string"/>
				        <xs:element name="size" type="xs:string"/>
				        <xs:element name="brand" type="xs:string"/>
				        <xs:element name="instock" type="xs:string"/>
						<xs:element name="contact" type="ty"/>
            			
				    </xs:sequence>
				</xs:complexType>
	        </xs:element>
	    </xs:sequence>
	</xs:complexType>
</xs:element>
<xs:simpleType name="ty">
<xs:restriction base="xs:integer">
<xs:minExclusive value="999999999"></xs:minExclusive>
<xs:maxExclusive value="10000000000"></xs:maxExclusive>
</xs:restriction>
</xs:simpleType>
</xs:schema>